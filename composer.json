{
    "name": "forwardjump/forwardjump-utility-core",
    "description": "This plugin combines functionality we use on multiple sites.",
    "keywords": [
        "wordpress"
    ],
    "type": "wordpress-plugin",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Tim Jensen",
            "homepage": "https://www.timjensen.us/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "webdevstudios/cmb2": "^2.2",
        "composer/installers": "~1.0"
    },
    "extra": {
        "installer-paths": {
            "vendor/CMB2/": [
                "webdevstudios/cmb2"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "src/custom-post-types/class-custom-post-type.php",
            "src/custom-taxonomies/class-custom-taxonomy.php",
            "src/genesis-admin-metaboxes/class-genesis-cmb2-admin-meta-box.php",
            "src/genesis-admin-metaboxes/class-genesis-cpt-archive-settings-meta-box.php",
            "src/genesis-admin-metaboxes/class-genesis-theme-settings-meta-box.php",
            "src/post-metaboxes/class-post-metabox.php",
            "src/settings/class-settings.php",
            "src/shortcodes/class-add-shortcode.php"
        ],
        "files": [
            "vendor/CMB2/init.php",
            "src/functions/apply-settings.php",
            "src/functions/gravity-forms.php",
            "src/functions/helper-functions.php",
            "src/functions/plugin.php",
            "src/settings/module.php",
            "src/shortcodes/module.php"
        ]
    }
}
